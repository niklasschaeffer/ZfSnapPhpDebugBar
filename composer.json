{
    "name": "niklasschaeffer/php-debug-bar",
    "description": "PHP Debug Bar module for Laminas",
    "keywords": [
        "debugbar",
        "zf2",
        "laminas",
        "debug",
        "exception",
        "log",
        "logger"
    ],
    "authors": [
        {
            "name": "Niklas Sch√§ffer",
            "email": "niklass.schaeffer@gmail.com",
            "homepage": "https://nickey.dev"
        }
    ],
    "require": {
        "php": "~8.0.0 || ~8.1.0",
        "ext-curl": "*",
        "ext-dom": "*",
        "ext-fileinfo": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "ext-pdo": "*",
        "ext-simplexml": "*",
        "ext-soap": "*",
        "maximebf/debugbar": "^1.10",
        "laminas/laminas-mvc": "3.7.0",
        "laminas/laminas-log": "^2.16",
        "laminas/laminas-servicemanager": "^3.20",
        "laminas/laminas-view": "^2.27",
        "laminas/laminas-eventmanager": "^3.11",
        "laminas/laminas-http": "^2.18",
        "laminas/laminas-modulemanager": "^2.14",
        "doctrine/dbal": "^2.13"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "laminas/laminas-config-aggregator": "^1.13",
        "laminas/laminas-serializer": "^2.17",
        "laminas/laminas-db": "^2.18"
    },
    "autoload": {
        "psr-4": {
            "PhpDebugBar\\": "src/"
        },
        "files": [
            "src/Functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "PhpDebugBar\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
